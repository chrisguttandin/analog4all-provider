{
    "extends": "tslint-config-holy-grail",
    "rules": {
        "component-selector": [ true, "element", "anp", "kebab-case" ],
        "directive-selector": [ true, "attribute", "anp", "camelCase" ],
        "import-blacklist": false,
        "no-implicit-dependencies": [ true, "dev" ],
        "pipe-naming": [ true, "camelCase", "anp" ]
    }
}
